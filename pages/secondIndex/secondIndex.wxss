/* pages/secondIndex/secondIndex.wxss */
.ucenter{
  width: 100%;
  height: 100%;
  padding-bottom: 80rpx;
  background: #f7f7f7;
  position: absolute;
  z-index: -2;
}

.pick {
  display: flex;
  /* flex-wrap 换行 */
  flex-wrap: wrap;
  height: 100%;
  /* 相对定位 */
  position: relative;
  background: #f5f6f7;
  padding: 0rpx 15rpx 30rpx 40rpx;
}

.pick .details {
  width: 320rpx;
  height: 480rpx;
  /* overflow图片使用自定义样式 */
  overflow: hidden;
  margin-top: 20rpx;
  margin-right: 29rpx;
  background: #fff;
  border-radius: 5px;
}

